{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "components/*": ["renderer/components/*"],
      "@components/*": ["renderer/components/*"],
      "@modules/*": ["renderer/modules/*"],
      "@hooks/*": ["renderer/hooks/*"],
      "lib/*": ["renderer/lib/*"],
      "pages-components/*": ["renderer/components/pages/*"],
      "@shared/*": ["shared/*"]
    }
  }
}
